[flake8]
exclude = 
    .git,
    __pycache__,
    venv,
    .venv,
    .pytest_cache,
    .mypy_cache,
    .vscode,
    build,
    dist,
    *.pyc,
    *.egg-info,
    .tox,
    .idea,
    .github,
    .githooks,
    assets,
    examples,
    portfolio_analysis_inputs,
    portfolio_analysis_output,
    scripts,
    tests,
    src/backend_projeto/api/openapi.json.client.ts,
    src/backend_projeto/api/openapi.json,
    src/backend_projeto/api/models,
    src/backend_projeto/api/deps.py,
    src/backend_projeto/api/main.py,
    src/backend_projeto/api/run.py,
    src/backend_projeto/api/__init__.py,
    src/backend_projeto/api/cache,
    src/backend_projeto/api/core,
    src/backend_projeto/api/outputs,
    src/backend_projeto/api/utils,
    src/backend_projeto/api/schemas.py,
    src/backend_projeto/api/generated.d.ts,
    src/backend_projeto/api/index.ts,
    src/backend_projeto/api/backend_projeto,
    src/backend_projeto/api/api,
    src/backend_projeto/api/auth,
    src/backend_projeto/api/data_endpoints.py,
    src/backend_projeto/api/factor_endpoints.py,
    src/backend_projeto/api/helpers.py,
    src/backend_projeto/api/optimization_endpoints.py,
    src/backend_projeto/api/portfolio_endpoints.py,
    src/backend_projeto/api/portfolio_simulation.py,
    src/backend_projeto/api/risk_endpoints.py,
    src/backend_projeto/api/system_endpoints.py,
    src/backend_projeto/api/technical_analysis_endpoints.py,
    src/backend_projeto/api/transaction_endpoints.py
max-line-length = 120
extend-ignore = E203, W503
