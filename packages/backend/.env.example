# .env.example
# Rename this file to .env and fill in the values.

# API Keys (Required)
FINNHUB_API_KEY=""
ALPHA_VANTAGE_API_KEY=""

# API Configuration
MAX_ASSETS_PER_REQUEST=100
REQUEST_TIMEOUT_SECONDS=300
ENABLE_CACHE=false
CACHE_TTL_SECONDS=3600
CACHE_DIR="cache"
GZIP_MINIMUM_SIZE=1000

# Logging
LOG_LEVEL="INFO"
LOG_FORMAT="text"

# Rate Limiting
RATE_LIMIT_ENABLED=false
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW_SECONDS=60
USE_REDIS_RATE_LIMITER=false

# CORS
CORS_ORIGINS="http://localhost:3000,http://127.0.0.1:3000"

# Data Provider
YFINANCE_TIMEOUT=30
YFINANCE_MAX_RETRIES=3
YFINANCE_BACKOFF_FACTOR=2.0
DATA_PROVIDER_MAX_RETRIES=3
DATA_PROVIDER_BACKOFF_FACTOR=2.0
DATA_PROVIDER_TIMEOUT=30

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
